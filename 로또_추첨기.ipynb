        "my_answer = list(map(int, my_answer))\n",
        "print(my_answer)\n",
        "\n",
        "lotto_answer = random.sample(range(1,46), k = 6)\n",
        "lotto_answer.sort()\n",
        "print(lotto_answer)\n",
        "\n",
        "answer_count = len(set(lotto_answer) & set(my_answer))\n",
        "print('맞춘 개수는: ', answer_count)\n",
        "\n",
        "if answer_count == 6:\n",
        "    print('1등입니다. 축하합니다. 당첨금: 20억')\n",
        "elif answer_count == 5:\n",
